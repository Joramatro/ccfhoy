	var pathname = window.location.pathname;
	var parts = pathname.split('/');
	$("#menuSuperior > li").removeClass('active');
	if(parts[1] == 'contacto'){		
		$("#menuContacto").addClass('active');
	}else if(parts[1] == 'sobre'){	
		$("#menuSobre").addClass('active');
